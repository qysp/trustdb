{
	"opts": {
		"readme": "./OVERVIEW.md",
		"destination": "./docs"
	},
	"tags": {
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["./lib/trustdb.js", "./lib/collection.js", "./lib/result.js", "./lib/schema.js"]
	},
  "plugins": ["plugins/markdown", "plugins/underscore"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}